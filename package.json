  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "concurrently -n nest,angular --kill-others \"npm run nest:start\" \"npm run angular:start\"",
    "build": "npm run nest:build && npm run angular:build",
    "test": "npm run nest:test && npm run angular:test",
    "nest:start": "nest start --debug --watch",
    "nest:build": "nest build",
    "nest:test": "jest",
    "nest:test:watch": "jest --watch",
    "nest:test:cov": "jest --coverage",
    "nest:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "nest:test:e2e": "jest --config ./test/nest/jest-e2e.json",
    "angular:start": "ng serve --disable-host-check",
    "angular:build": "ng build",
    "angular:test": "ng test",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@angular/animations": "^14.0.1",
    "@angular/cdk": "^14.0.1",
    "@angular/common": "^14.0.1",
    "@angular/compiler": "^14.0.1",
    "@angular/core": "^14.0.1",
    "@angular/forms": "^14.0.1",
    "@angular/material": "^14.0.1",
    "@angular/platform-browser": "^14.0.1",
    "@angular/platform-browser-dynamic": "^14.0.1",
    "@angular/router": "^14.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "js-yaml": "^4.1.0",
    "nano": "^10.0.0",
    "ngx-mask": "^14.0.3",
    "passport": "^0.5.2",
